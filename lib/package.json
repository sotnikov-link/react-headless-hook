{
  "name": "react-headless-hook",
  "description": "Hook or Types for React Headless Components",
  "keywords": [
    "react",
    "headless",
    "hook",
    "typescript",
    "types"
  ],
  "files": [
    "src/**",
    "dist/main/**",
    "dist/module/**",
    "README.md"
  ],
  "packageManager": "yarn@3.2.1",
  "version": "0.2.0",
  "source": "src/index.ts",
  "types": "dist/main/index.d.ts",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "scripts": {
    "prepare": "run-p -l 'prepare-*' && echo prepared",
    "prepare-es5cjs": "tsc",
    "prepare-module": "tsc --project tsconfig.module.json",
    "start": "run-p -l 'prepare-* --watch'",
    "clean": "rm -rf dist",
    "prepack": "yarn clean && yarn prepare && cp ../README.md ./",
    "postpack": "rm ./README.md"
  },
  "dependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@types/react": ">=16",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4"
  }
}
